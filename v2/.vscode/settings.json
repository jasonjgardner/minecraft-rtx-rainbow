{
    "deno.importMap": "./import_map.json",
    "deno.config": "./deno.json",
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": true,
    "editor.defaultFormatter": "denoland.vscode-deno",
    "nuxt.isNuxtApp": false,
    "cSpell.words": [
        "flipbook",
        "flipbooks"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "editor.formatOnSave": true
}